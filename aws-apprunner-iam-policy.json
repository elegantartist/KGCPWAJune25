{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "KGCSecretsManagerAccess",
      "Effect": "Allow",
      "Action": [
        "secretsmanager:GetSecretValue",
        "secretsmanager:DescribeSecret"
      ],
      "Resource": [
        "arn:aws:secretsmanager:us-east-1:*:secret:prod/KGC/sec*"
      ]
    }
  ]
}